{
  "name": "backslash",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "cache-flush": "aws cloudfront create-invalidation --distribution-id E13WE8G6L89PC5 --paths '/*'",
    "db-handler:deploy": "./scripts/db-handler-deploy.sh",
    "db-handler:deploy-dev": "./scripts/db-handler-deploy-dev.sh",
    "db-handler:deploy-prod": "./scripts/db-handler-deploy-prod.sh",
    "deploy": "tsc && vite build && aws s3 cp ./dist/ s3://backslash-app.com/ --recursive",
    "dev": "vite",
    "fix": "npm run format && npm run sort-package-jsons",
    "format": "prettier --write '**/*.{ts,tsx,md,json,jsx,css}'",
    "preview": "vite preview",
    "sort-package-jsons": "sort-package-json package.json src/*/package.json",
    "test": "jest"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-swipeable": "^7.0.0"
  },
  "devDependencies": {
    "@types/google-one-tap": "^1.2.0",
    "@types/jest": "^28.1.3",
    "@types/json-schema": "^7.0.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "aws-sdk": "^2.1167.0",
    "jest": "^28.1.1",
    "json-schema": "^0.4.0",
    "prettier": "^2.7.1",
    "sort-package-json": "^1.57.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
