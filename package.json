{
  "name": "backslash",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "auth-handler:deploy": "./scripts/auth-handler-deploy.sh",
    "auth-handler:deploy-dev": "./scripts/auth-handler-deploy-dev.sh",
    "auth-handler:deploy-prod": "./scripts/auth-handler-deploy-prod.sh",
    "build": "tsc && vite build",
    "cache-flush": "aws cloudfront create-invalidation --distribution-id E13WE8G6L89PC5 --paths '/*'",
    "db-handler:deploy": "./scripts/db-handler-deploy.sh",
    "db-handler:deploy-dev": "./scripts/db-handler-deploy-dev.sh",
    "db-handler:deploy-prod": "./scripts/db-handler-deploy-prod.sh",
    "deploy": "tsc && vite build && aws s3 cp ./dist/ s3://backslash-app.com/ --recursive",
    "deploy:flush": "npm run deploy && npm run cache-flush",
    "dev": "npm run start & npm run tsc:watch",
    "fix": "npm run format && npm run sort-package-jsons && npm run lint:fix",
    "format": "prettier --write '**/*.{ts,tsx,md,json,jsx,css}'",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "sort-package-jsons": "sort-package-json package.json src/*/package.json",
    "start": "vite",
    "test": "jest",
    "tsc:watch": "tsc --watch --noEmit"
  },
  "dependencies": {
    "@types/lodash.throttle": "^4.1.7",
    "lodash.throttle": "^4.1.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^4.4.0",
    "react-swipeable": "^7.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/google-one-tap": "^1.2.0",
    "@types/jest": "^28.1.3",
    "@types/json-schema": "^7.0.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@vitejs/plugin-react": "^1.3.0",
    "aws-sdk": "^2.1167.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^28.1.1",
    "json-schema": "^0.4.0",
    "prettier": "^2.7.1",
    "sort-package-json": "^1.57.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
